package simce;

import java.util.Scanner;



/**
 *
 * @author dci
 */
public class Simce {

    /**
     * @param args the command line arguments
     */
    public static void main(String[] args) {
        // TODO code application logic here
        
        Scanner entrada = new Scanner(System.in);
        
        int CantCol;
        int CantAlum;
        int puntaje;
        int bueno = 0;
        int porcentaje;
        int insuficiente = 0;
        int suficiente = 0;
        int prominsu;
        int prombueno;
        int promeTotalInsu = 0 ;
        int promTotalBueno = 0 ;
        int promTotalsufi = 0;

        System.out.println("Ingrese cantidad de establecimientos");
        CantCol = entrada.nextInt();

        for (int i = 0; i < CantCol; i++) {

            System.out.println("Ingrese cantidad de estudiantes");
            CantAlum = entrada.nextInt();

            for (int c = 0; c < CantAlum; c++) {
                System.out.println("Ingrese puntaje");
                puntaje = entrada.nextInt();
               
                if (puntaje < 300) {

                    insuficiente = insuficiente + 1;
                 
                }
                    if  (puntaje > 500) {
                    
                    bueno = bueno + 1;

                    }
     
            
     }
        
        prominsu = insuficiente / CantAlum * 100;
                prombueno = bueno / CantAlum * 100;

                if (prominsu > 70) {
                    promeTotalInsu = promeTotalInsu + 1;
                    System.out.println("Insuficiente");
                }else{
                if (prombueno > 70) {
                    promTotalBueno = promTotalBueno + 1;
                    System.out.println("Bueno");
                }else{
                    promTotalsufi = promTotalsufi + 1;
                    System.out.println("Suficiente");
                }/*
